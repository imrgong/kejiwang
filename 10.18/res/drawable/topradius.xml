<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="false" android:drawable="@color/white" >
      <shape>
            <solid android:color="@color/label_pressed" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />
        </shape>
  </item>
    <item android:state_selected="true" android:drawable="@color/label_pressed" >
        <shape>
            <solid android:color="@color/label_pressed" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true" android:drawable="@color/label_pressed" >
        <shape>
            <solid android:color="@color/label_pressed" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:drawable="@color/home_bg_color" >
         <shape>
            <solid android:color="@color/label_pressed" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" />
        </shape>
    </item>
</selector>
